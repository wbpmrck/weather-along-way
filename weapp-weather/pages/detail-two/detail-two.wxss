/* 顶部和底部信息区域切换动画效果 */
.top-move-down {
  animation: ani-top-move-down 0.8s ease-in;
}

.down-move-top {
  transform: translateY(-100%);
  animation: ani-down-move-top 0.8s ease-in;
}

@keyframes ani-top-move-down {
  from {
    transform: translateY(-100%);
  }

  to {
    transform: translateY(0);
  }
}

@keyframes ani-down-move-top {
  from {
    transform: translateY(0);
  }

  to {
    transform: translateY(-100%);
  }
}

/* 城市详情模式-顶部信息区域 */
.city-detail {
  position: absolute;
  z-index: 100;
  top: 0;
  width: 100%;
  background-color: #fff;
}

.city-detail .top {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding: 5rpx 40rpx 0 40rpx;
}

.city-detail .district {
  display: inline-block;
  font-size: 48rpx;
  font-weight: bold;
  margin-right: 12rpx;
  /* min-width: 100rpx; */
}

.city-detail .city {
  display: inline-block;
  font-size: 32rpx;
}

.city-detail .today-region {
  border-radius: 12rpx;
  padding: 12rpx;
}

.city-detail .today {
  font-size: 32rpx;
}

.city-detail .today-weather {
  font-size: 36rpx;
}

/* 天气数据区域 */

.city-detail .weather-data {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-top: 30rpx;
  padding: 5rpx 40rpx 0 40rpx;
}

.city-detail .weather-data .label {
  font-size: 30rpx;
  color: #808080;
  margin-bottom: 20rpx;
}

.city-detail .arrive-time-region {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.city-detail .arrive-weather-region {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.city-detail .arrive-weather-region image {
  /* .city-detail .arrive-weather-region cover-image { */
  width: 48rpx;
  height: 48rpx;
}

.city-detail .arrive-weather-region .content {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.city-detail .not-good-region {
  display: flex;
  flex-direction: column;
  align-items: center;
}

/* 预警区域 */
.city-detail .alarms {
  margin-top: 32rpx;
}

.city-detail .alarm {
  font-size: 38rpx;
  line-height: 80rpx;
  height: 80rpx;
  text-align: center;
  font-weight: normal;
  margin-top: 1rpx;
}

/* 白色预警 */
.city-detail .alarm-00 {
  background-color: #fff;
}

/* 蓝色预警 */
.city-detail .alarm-01 {
  background-color: #65A1FF;
}

/* 黄色预警 */
.city-detail .alarm-02 {
  background-color: #F9E79F;
}

/* 橙色预警 */
.city-detail .alarm-03 {
  background-color: #E67E22;
}

/* 红色预警 */
.city-detail .alarm-04 {
  background-color: #EC7063;
}

.hidden {
  visibility: hidden;
}