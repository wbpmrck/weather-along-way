.weather-summary {
  position: absolute;
  z-index: 100;
  top: 0;
  width: 100%;
  background-color: #fff;
  padding-top: 20rpx;
  padding-bottom: 10rpx;
}

.weather-summary .summary-item {
  display: flex;
  align-items: center;
  justify-content: space-around;
  margin-top: 22rpx;
  margin-bottom: 22rpx;
}

.fix-width-1 {
  width: 300rpx;
  text-align: center;
}

.weather-summary .count {
  font-weight: 600;
  font-size: 48rpx;
}

.weather-summary .text {
  /* margin: 32rpx 0; */
}

.weather-summary .distance {
  font-size: 32rpx;
}

.share-route {
  display: inline-block;
  border-radius: 48rpx;
  margin: 0 0 0 0;
  width: 188rpx;
  height: 58rpx;
  line-height: 58rpx;
  font-size: 32rpx;
  /* margin: 48rpx; */
  background-image: url('data:image/png;base64,');
  background-size: cover;
  color: #fff;
}

.share-route::after {
  border: 0;
}

.share-wrapper {
  position: relative;
  height: 35px;
}

#share-btn {
  position: absolute;
  box-sizing: border-box;
  z-index: 110;
  width: 120px;
  height: 35px;
  /* line-height: 35px; */
  margin: none;
  padding: none;
  /* font-size: 18px; */
  left: 50%;
  top: 50%;
  transform: translateX(-50%) translateY(-50%);
  border-radius: 20px;
}

.share-img {
  position: absolute;
  z-index: 210;
  /* width: 120px; */
  width: 100%;
  height: 35px;
  left: 50%;
  top: 50%;
  transform: translateX(-50%) translateY(-50%);
}

.inner {
  position: absolute;
  z-index: 200;
  color: #fff;
  left: 50%;
  top: 50%;
  transform: translateX(-50%) translateY(-50%);
}

.map {
  width: 100%;
  height: 100vh;
  /* height: 750rpx; */
}


.bottom-region {
  position: absolute;
  z-index: 100;
  bottom: 0;
  width: 100%;
  background-color: #fff;
}

.return {
  position: absolute;
  top: -50px;
  right: 10px;
  z-index: 200;
  width: 40px;
  height: 40px;
}

.arrange {
  padding: 24rpx;
  text-align: center;
}

.weather-stat {
  display: flex;
  justify-content: space-between;
  margin: 10rpx 10rpx;
  text-align: center;
}

.select-route {
  background-color: #65A1FF;
  color: #fff;
}

.stat-item {
  padding: 16rpx;
  font-size: 32rpx;
  border-radius: 16rpx;
}

.stat-item .title {
  font-weight: 600;
}

.stat-item .duration {
  margin: 16rpx 0;
}

